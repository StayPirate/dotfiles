alias sles15='podman run --rm -ti \
-v $HOME/SUSE/.secret/SCCcredentials:/etc/zypp/credentials.d/SCCcredentials \
-e ADDITIONAL_MODULES="\
sle-module-basesystem,\
sle-module-desktop-applications,\
sle-module-development-tools,\
sle-module-server-applications,\
sle-module-legacy,\
sle-module-public-cloud,\
sle-module-web-scripting,\
sle-module-containers,\
sle-module-cap-tools,\
sle-module-live-patching,\
SLES-LTSS" \
registry.suse.com/suse/sle15:15.0 \
/bin/bash'

